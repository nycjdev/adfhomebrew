<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE html>
<f:view xmlns:f="http://java.sun.com/jsf/core" xmlns:af="http://xmlns.oracle.com/adf/faces/rich"
        xmlns:dvt="http://xmlns.oracle.com/dss/adf/faces">
    <af:document title="testgraph.jsf" id="d1">
        <af:form id="f1">
            <dvt:diagram id="dvt1" summary="Example Diagram" layout="circleLayout">
                <dvt:clientLayout name="circleLayout" method="DemoCircleLayout.circleLayout"
                                  featureName="DemoCircleLayout"/>
                <dvt:diagramNodes id="dns1" value="#{TestGraphModel.nodes}" var="node">
                    <dvt:diagramNode id="dn1" nodeId="#{node.uniqueNodeId}" label="#{node.nodeLabel}">
                        <f:facet name="zoom100">
                            <dvt:marker shape="circle" height="20" width="20" fillColor="#ff0000" borderColor="#000000"
                                        borderStyle="solid" borderWidth="1.0"/>
                        </f:facet>
                    </dvt:diagramNode>
                </dvt:diagramNodes>
                <dvt:diagramLinks id="dls1" value="#{TestGraphModel.links}" var="link">
                    <dvt:diagramLink id="dl1" startNode="#{link.sourceNodeId}" endNode="#{link.destinationNodeId}"
                                     startConnectorType="circle" endConnectorType="arrowOpen"/>
                </dvt:diagramLinks>
            </dvt:diagram>
        </af:form>
    </af:document>
</f:view>